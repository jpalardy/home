{
  "author": "Jonathan Palardy",
  "name": "home",
  "description": "The web pages at home.jpalardy.com",
  "version": "0.3.0",
  "scripts": {
    "build": "parcel build src/html/index.html -d public --public-url ./ --no-minify --no-cache",
    "watch": "parcel serve src/html/index.html -d public --no-source-maps",
    "coverage": "nyc --reporter=html --reporter text npm test",
    "test": "mocha --reporter dot"
  },
  "devDependencies": {
    "less": "3.0.1",
    "mocha": "5.0.1",
    "nyc": "11.4.1",
    "parcel-bundler": "1.6.2"
  },
  "private": true
}
